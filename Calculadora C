   #include <stdio.h>;

    int main (){

         //  Variaveis

        int num;
        int num2;
        int soma;
        int subtracao;
        int multiplicacao;
        int divisao;
    {
            // Exibir texto
  
    printf ("Digite o primeiro numero:\n");
    scanf ("%d", &num);
    printf ("Digite o segundo numero:\n");
    scanf ("%d", &num2);

         // Variaveis resultados

    soma = num + num2;
    subtracao = num - num2;
    multiplicacao = num * num2;
    divisao = num / num2;
       
          // Exibir resultado na tela
  
    printf("\nResultado soma: \n %d + %d = %d" ,num ,num2 ,soma);
    printf("\nResultado subtracao: \n %d - %d = %d" ,num ,num2 ,subtracao);
    printf("\nResultado multiplicacao: \n %d * %d = %d" ,num ,num2 ,multiplicacao);
    printf("\nResultado divisao: \n %d / %d = %d" ,num ,num2 ,divisao);
    }
}
